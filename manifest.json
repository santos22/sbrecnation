{
  "manifest_version": 2,
  "name": "SBRecNation",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.1.1.min.js", "jquery.scrollTo.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "large_managingmadrid.com.minimal.png"
  }
}